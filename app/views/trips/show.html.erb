<p id="notice"><%= notice %></p>

<p>
  <strong>Location:</strong>
  <%= @trip.location %>
</p>

<p>
  <strong>Trip start date:</strong>
  <%= @trip.trip_start_date %>
</p>

<p>
  <strong>Trip end date:</strong>
  <%= @trip.trip_end_date %>
</p>

<p>
  <strong>Description:</strong>
  <%= @trip.description %>
</p>

<% @results.each do |venue| %>
  <%= venue["name"] %><%= venue["lat"] %><%= venue["long"] %><br />
<% end %>

<iframe
  width="600"
  height="450"
  frameborder="0" style="border:0"
  src="https://www.google.com/maps/embed/v1/place?key=AIzaSyBpE2piRO8YAg6j1kFbcOkMGuupn1M1AnQ
    &q=<%= @results.first["name"] %>">
</iframe>

<%= link_to 'Edit', edit_trip_path(@trip) %> |
<%= link_to 'Back', trips_path %>
